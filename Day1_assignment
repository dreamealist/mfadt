   void setup() {
     size (500, 500);
     
   }
   
   void draw() {
     background(0);
     noStroke();
     
   //background rectangle
   fill(246, 152, 153); 
   rect(0, 0, 100, 100);
   fill(134, 184, 85);
   rect(100, 100, 100, 100);
   fill(246, 152, 153);
   rect(200, 200, 100, 100);
   fill(126, 55, 185);
   rect(300, 300, 100, 100);
   fill(246, 152, 153);
   rect(400, 400, 100, 100);
   
   
   //Other side
   fill(215, 255, 71); 
   rect(400, 0, 100, 100);
   fill(246, 152, 153);
   rect(0, -400, 400, 100);
   
   rect(200, 0, 100, 100);
   
   rect(400, 200, 100, 100);
   fill(246, 152, 153);
   rect(400, 0, 100, 100);
   fill(134, 184, 85);
   rect(0, 400, 100, 100);
   fill(255, 105, 71);
   rect(0, 200, 100, 100);
   
   rect(200, 400, 100, 100);
   fill(134, 184, 85);
   rect(100, 300, 100, 100);
   fill(55, 111, 185);
   rect(400, 0, 100, 100);
   fill(55, 111, 185);
   rect(300, 100, 100, 100);
   
   //dot

   fill(255, 223, 1);
   ellipse(400, 0, 100, 100);
   ellipse(400, 200, 50, 50);
   ellipse(300, 200, 80, 80);
   ellipse(350, 300, 40, 40);
   ellipse(250, 100, 20, 20);
   ellipse(250, 330, 50, 50);
   ellipse(380, 100, 35, 35);
   
   //flower
   fill(55, 111, 185);
   rect(50, 400, 50, 50);
   fill(55, 111, 185);
   rect(100, 350, 50, 50);
   fill(55, 111, 185);
   rect(150, 400, 50, 50);
   fill(55, 111, 185);
   rect(100, 450, 50, 50);
   fill(55, 111, 185);
   rect(100, 400, 50, 50);
   fill(248, 248, 248);
   rect(120, 360, 10, 50);
   fill(248, 248, 248);
   rect(120, 440, 10, 50);
   fill(248, 248, 248);
   rect(140, 420, 50, 10);
   fill(248, 248, 248);
   rect(60, 420, 50, 10);
   fill(248, 248, 248);
   rect(60, 420, 50, 10);
   
  //line
   fill(248, 248, 248);
   rect(100, 0, 100, 25);
   fill(248, 248, 248);
   rect(100, 50, 100, 25);
   
   //cross
   fill(248, 248, 248);
   rect(400, 335, 100, 25);
   fill(248, 248, 248);
   rect(438, 300, 25, 100);
   
   }

